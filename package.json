{
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "aws-sdk": "^2.1692.0",
    "d-fi": "^2.1.6",
    "d-fi-core": "^1.3.5",
    "decimal.js": "^10.5.0",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "js-sha256": "^0.11.0",
    "meyda": "^5.6.3",
    "music-tempo": "^1.0.3",
    "mysql2": "^3.12.0",
    "nodemailer": "^6.10.0",
    "sqlstring": "^2.3.3",
    "uuid": "^11.0.5",
    "web-audio-api": "^0.2.2",
    "winston": "^3.17.0"
  },
  "name": "deezar",
  "version": "1.0.0",
  "main": "node/index.js",
  "bin": {
    "deezar": "dist/index.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_OPTIONS=--openssl-legacy-provider ts-node index.ts",
    "clean": "rm -rf node_modules && rm -rf package-lock.json && rm -f *.log && rm -rf dist && rm -f deezar-linux && rm -f deezar-macos && rm -f deezar-win.exe && rm -f deezar-macos",
    "build": "npx tsc && cp -r assets dist/",
    "nexe:linux": "nexe dist/index.js -t linux-x64-20.16.0 -o deezar-linux --build",
    "nexe:macos": "nexe dist/index.js -t mac-x64-20.16.0 -o deezar-macos --build",
    "nexe:win": "nexe dist/index.js -t windows-x64-20.16.0 -o deezar-win.exe --build",
    "nexe": "npm run build && npm run nexe:linux && npm run nexe:macos && npm run nexe:win"
  },
  "author": "Pierric Buchez",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@types/express": "^5.0.0",
    "@types/node": "^24.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/sqlstring": "^2.3.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "nexe": "^5.0.0-beta.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "pkg": {
    "targets": [
      "node-linux-x64",
      "node-macos-x64",
      "node-win-x64"
    ]
  }
}
