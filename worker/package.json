{
  "name": "worker",
  "version": "1.0.0",
  "description": "worker of epico",
  "license": "MIT",
  "author": "MyEcoria",
  "type": "commonjs",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_OPTIONS=--openssl-legacy-provider ts-node index.ts",
    "build": "npx tsc",
    "nexe:linux": "nexe dist/index.js -t linux-x64-24.2.0 -o worker --build",
    "nexe:macos": "nexe dist/index.js -t mac-x64-24.2.0 -o worker --build",
    "nexe:win": "nexe dist/index.js -t windows-x64-24.2.0 -o worker.exe --build",
    "nexe": "npm run build && npm run nexe:linux && npm run nexe:macos && npm run nexe:win"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.828.0",
    "axios": "^1.10.0",
    "bull": "^4.16.5",
    "d-fi-core": "^1.3.5",
    "dotenv": "^16.5.0",
    "music-tempo": "^1.0.3",
    "mysql2": "^3.14.1",
    "ts-node": "^10.9.2",
    "uuid": "^11.1.0",
    "web-audio-api": "^0.2.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0"
  }
}
